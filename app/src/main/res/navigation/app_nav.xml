<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_nav"
    app:startDestination="@id/dashBoardFragment">
    <fragment
        android:id="@+id/dashBoardFragment"
        android:name="com.bugbd.DashBoardFragment"
        android:label="fragment_dash_board"
        tools:layout="@layout/fragment_dash_board" >
        <action
            android:id="@+id/action_dashBoardFragment_to_mainTabFragment"
            app:destination="@id/mainTabFragment" />
    </fragment>
    <fragment
        android:id="@+id/mainTabFragment"
        android:name="com.bugbd.code.main.MainTabFragment"
        android:label="fragment_main_tab"
        tools:layout="@layout/fragment_main_tab" />
</navigation>